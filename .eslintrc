{
  "plugins": [
    "angular"
  ],
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "angular/di": [
      2,
      "$inject"
    ],
    "angular/no-service-method": "off",
    "angular/module-getter": "off"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "globals": {
    "emit": true
  }
}
